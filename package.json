{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript api boilerplate",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "lint": "prettier --write .",
    "build": "npx tsc",
    "prisma:init": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate-init": "npx prisma migrate dev --name init",
    "deploy:prod": "npm i && npm run prisma:init && npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JesseLD/Typescript-Api.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "Api",
    "Rest",
    "Typescript",
    "Javascript",
    "Node"
  ],
  "author": "JesseLD",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JesseLD/Typescript-Api/issues"
  },
  "homepage": "https://github.com/JesseLD/Typescript-Api#readme",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@types/sequelize": "^4.28.20",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1"
  }
}
